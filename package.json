{
  "name": "@hastobegood/crypto-bot-bloodhound",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "^14"
  },
  "scripts": {
    "clean": "rm -rf dist .aws-sam reports",
    "build": "yarn tsc",
    "bundle": "yarn webpack",
    "bundle:analyze": "yarn webpack && yarn webpack-bundle-analyzer dist/webpack/stats.json",
    "test": "yarn jest",
    "test:coverage": "yarn jest --collectCoverage",
    "lint": "yarn eslint . --ext .ts",
    "lint:fix": "yarn eslint . --ext .ts --fix",
    "lint:report": "yarn eslint . --ext .ts --format json -o reports/eslint-report.json ",
    "deploy:prod": "sam build && sam deploy --config-env prod"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.67.0",
    "@aws-sdk/client-eventbridge": "^3.67.0",
    "@aws-sdk/client-secrets-manager": "^3.67.0",
    "@aws-sdk/client-sns": "^3.67.0",
    "@aws-sdk/lib-dynamodb": "^3.67.0",
    "@hastobegood/crypto-clients-binance": "^0.6.0",
    "aws-xray-sdk-core": "^3.3.4",
    "axios": "^0.24.0",
    "keyv": "^4.0.5",
    "lodash": "^4.17.21",
    "node-html-parser": "^5.2.0",
    "pino": "^7.10.0",
    "serialize-error": "^9.0.0",
    "source-map-support": "^0.5.21",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.93",
    "@types/copy-webpack-plugin": "^8.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.3",
    "@types/jsdom": "^16.2.13",
    "@types/keyv": "^3.1.3",
    "@types/lodash": "^4.14.178",
    "@types/node": "^14.18.0",
    "@types/pino": "^7.0.5",
    "@types/randomstring": "^1.1.7",
    "@types/source-map-support": "^0.5.4",
    "@types/uuid": "^8.3.4",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@types/webpack-node-externals": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.18.0",
    "copy-webpack-plugin": "^10.0.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fs-extra": "^10.0.0",
    "jest": "^27.5.1",
    "jest-sonar-reporter": "^2.0.0",
    "mockdate": "^3.0.5",
    "prettier": "^2.5.1",
    "randomstring": "^1.2.1",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.3.0",
    "typescript": "^4.5.2",
    "webpack": "^5.64.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  },
  "jestSonar": {
    "reportPath": "reports"
  }
}
